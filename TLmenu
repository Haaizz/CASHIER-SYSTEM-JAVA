		originalpearlselect = new JRadioButton("OP");
		originalpearlselect.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent e) 
			{
				if(originalpearlselect.isSelected())
				{
					textField.setEnabled(true);
					textField.setText("");
					textField.requestFocus();
				}
				
				else
				{
					textField.setEnabled(false);
					textField.requestFocus();
				}
				
			}
		});
		
